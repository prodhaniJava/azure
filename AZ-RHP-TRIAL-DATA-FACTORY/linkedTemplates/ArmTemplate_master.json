{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AZ-RHP-TRIAL-DATA-FACTORY"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureMySql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySql1'"
		},
		"azrhpemplyeeblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azrhpemplyeeblob'"
		},
		"emplyeecsvblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'emplyeecsvblob'"
		},
		"lsemployeecsvblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsemployeecsvblob'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AZ-RHP-TRIAL-DATA-FACTORY_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureMySql1_connectionString": {
						"value": "[parameters('AzureMySql1_connectionString')]"
					},
					"azrhpemplyeeblob_connectionString": {
						"value": "[parameters('azrhpemplyeeblob_connectionString')]"
					},
					"emplyeecsvblob_connectionString": {
						"value": "[parameters('emplyeecsvblob_connectionString')]"
					},
					"lsemployeecsvblob_connectionString": {
						"value": "[parameters('lsemployeecsvblob_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}